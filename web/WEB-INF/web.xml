<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <display-name>search</display-name>

    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
        <welcome-file>LMSearch.html</welcome-file>
    </welcome-file-list>


    <servlet>
        <servlet-name>LMSearchServlet</servlet-name>
        <servlet-class>cn.edu.seu.servlet.LMSearchServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LMSearchServlet</servlet-name>
        <url-pattern>/LMSearchServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>OtherSearchServlet</servlet-name>
        <servlet-class>cn.edu.seu.servlet.OtherSearchServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>OtherSearchServlet</servlet-name>
        <url-pattern>/OtherSearchServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SIMSearchServlet</servlet-name>
        <servlet-class>cn.edu.seu.servlet.SIMSearchServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SIMSearchServlet</servlet-name>
        <url-pattern>/SIMSearchServlet</url-pattern>
    </servlet-mapping>

</web-app>